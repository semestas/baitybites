{
  "name": "baitybites",
  "version": "1.1.0",
  "description": "Order Management System for BaityBites",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run style:watch\"",
    "dev:server": "bun --watch index.ts",
    "start": "bun index.ts",
    "build": "bun run style:build && bun build index.ts --outdir ./dist --target bun",
    "style:watch": "sass --watch src/scss/main.scss public/css/main.css",
    "style:build": "sass src/scss/main.scss public/css/main.css --style compressed"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "concurrently": "^9.2.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "prettier": "^3.8.0",
    "sass": "^1.97.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/static": "^1.4.7",
    "arctic": "^3.7.0",
    "elysia": "^1.4.22",
    "elysia-oauth2": "^2.1.0",
    "postgres": "^3.4.8"
  }
}