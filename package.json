{
  "name": "baitybites",
  "version": "2.2.0",
  "description": "Order Management System for Baitybites",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build:html": "bun build-html.ts",
    "update-version": "bun build-html.ts",
    "dev": "bun run build:html && concurrently \"bun run dev:server\" \"bun run style:watch\" \"bun --watch build-html.ts\"",
    "dev:server": "bun --watch index.ts",
    "start": "bun run build:html && bun index.ts",
    "build": "bun run build:html && bun run style:build && bun build index.ts --outdir ./dist --target bun",
    "style:watch": "sass --watch src/scss/main.scss:public/css/main.css src/scss/style.scss:public/css/style.css src/scss/email.scss:public/css/email.css src/scss/wa-direct.scss:public/css/wa-direct.css",
    "style:build": "sass src/scss/main.scss:public/css/main.css src/scss/style.scss:public/css/style.css src/scss/email.scss:public/css/email.css src/scss/wa-direct.scss:public/css/wa-direct.css --style compressed"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/crypto-js": "^4.2.2",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "concurrently": "^9.2.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "prettier": "^3.8.0",
    "sass": "^1.97.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/jwt": "^1.4.0",
    "@google/generative-ai": "^0.24.1",
    "@types/nodemailer": "^7.0.9",
    "arctic": "^3.7.0",
    "cloudinary": "^2.9.0",
    "crypto-js": "^4.2.0",
    "elysia": "^1.4.22",
    "elysia-oauth2": "^2.1.0",
    "nodemailer": "^8.0.1",
    "oauth-1.0a": "^2.2.6",
    "postgres": "^3.4.8",
    "puppeteer": "^24.37.3"
  }
}